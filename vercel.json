{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/register",
      "dest": "server/index.js"
    },
    {
      "src": "/login",
      "dest": "server/index.js"
    },
    {
      "src": "/load-progress",
      "dest": "server/index.js"
    },
    {
      "src": "/(.*\\.(js|css|html|jpg|jpeg|png|gif|gltf|bin|json))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ]
}

